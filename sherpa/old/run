# B
mpirun -np 5 Sherpa2.2.0 -f Run_TTYY_HT2_13TeV.dat EVENTS=0 \$NLOPART:=B \$BENERGY:=6500 PSI_ITMIN=100000 \$RNDSEED1:=050 \$RNDSEED2:=050 | tee Log/grid_B_13TeV_r050-050.log
# I
mpirun -np 5 Sherpa2.2.0 -f Run_TTYY_HT2_13TeV.dat EVENTS=0 \$NLOPART:=I \$BENERGY:=6500 PSI_ITMIN=100000 \$RNDSEED1:=050 \$RNDSEED2:=050 | tee Log/grid_I_13TeV_r050-050.log
# V
mpirun -np 5 Sherpa2.2.0 -f Run_TTYY_HT2_13TeV.dat EVENTS=0 \$NLOPART:=V \$BENERGY:=6500 \$LOOPGEN:=Internal PSI_ITMIN=100000 \$RNDSEED1:=050 \$RNDSEED2:=050 | tee Log/grid_V_13TeV_r050-050.log
# RS
mpirun -np 5 Sherpa2.2.0 -f Run_TTYY_HT2_13TeV.dat EVENTS=0 \$NLOPART:=RS \$BENERGY:=6500 PSI_ITMIN=100000 \$RNDSEED1:=050 \$RNDSEED2:=050 | tee Log/grid_RS_13TeV_r050-050.log
